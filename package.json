{
  "name": "cloud-game-driver",
  "private": true,
  "version": "1.0.0",
  "description": "A cloud game driver and automator",
  "scripts": {
    "test": "NODE_DEBUG=cloud-game-driver nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=cloud-game-driver nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "posttest": "npm run report",
    "report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html",
    "start": "electron src/games/letsgo/index.js"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/cloud-game-driver.git"
  },
  "keywords": [
    "cloud-game-driver"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/cloud-game-driver/issues"
  },
  "devDependencies": {
    "@ostai/eslint-config": "^4.0.0",
    "ava": "^6.2.0",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "nyc": "^17.1.0"
  },
  "dependencies": {
    "electron": "^34.0.1",
    "express": "^4.18.2",
    "pngjs": "^7.0.0",
    "puppeteer": "^22.0.0",
    "ssim.js": "^3.5.0"
  }
}
